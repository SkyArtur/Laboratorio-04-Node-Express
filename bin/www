#!/usr/lib/env node
const app = require('../app')
const http = require('http')
const normalizaPort = (value) => {
    let port = value
    if (isNaN(port)) return value
    if (port >= 0) return port
    return false
}
const port = normalizaPort(process.env.PORT || 3000)

app.set('port', port)

const server = http.createServer(app)
server.listen(port)
